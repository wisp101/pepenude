    "gpu-info": "^0.0.1",
    "image-to-base64": "^2.0.1",
    "js-event-bus": "^1.0.0",
    "lodash": "^4.17.11",
    "markdown": "^0.5.0",
    "md5": "^2.2.1",
    "mime-types": "^2.1.24",
    "moment": "^2.24.0",
    "nuxt": "^2.0.0",
    "queue": "^6.0.1",
