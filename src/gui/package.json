{
  "name": "dreamtime-gui",
  "version": "0.1.1-alpha",
  "description": "",
  "author": "DeepManYY",
  "private": true,
  "main": "electron/main.js",
  "build": {
    "appId": "com.deepmanyy.dreamtime",
    "productName": "DreamTime",
    "directories": {
      "output": "../../dist/gui-unpacked"
    },
    "win": {
      "target": "dir",
      "icon": "app.ico"
    },
    "linux": {
      "target": "dir",
      "icon": "app.ico",
      "executableName": "DreamTime"
    }
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "yarn lint",
    "dev": "cross-env NODE_ENV=dev nuxt",
