    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/eslint-module": "^0.0.1",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "electron": "^5.0.6",
